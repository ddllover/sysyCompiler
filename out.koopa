fun @main(): i32 {
%entry:
  @x = alloc i32

  @y = alloc i32
  store 21, @y

  %0 = load @y
  %1 = add %0, 1
  store %1, @x

  @z = alloc i32
  %2 = load @x
  %3 = sub 147, %2
  store %3, @z

  @w = alloc i32
  store 147, @w

  %4 = load @w
  %5 = mul %4, 1
  %6 = mul %5, 1
  %7 = mul %6, 1
  store %7, @w

  %8 = load @z
  %9 = load @w
  %10 = add %8, %9
  ret %10
}
