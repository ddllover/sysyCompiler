decl @getint(): i32
decl @getch(): i32
decl @getarray(*i32): i32
decl @putint(i32)
decl @putch(i32)
decl @putarray(i32, *i32)
decl @starttime()
decl @stoptime()

fun @main():i32 {
%entry:
  %ret = alloc i32
  jump %begin

%begin:
  %0 = add 0, 4
  %1 = sub %0, 1
  store %1, %ret

  jump %end

%ret_1:
  jump %end

%end:
  %2 = load %ret
  ret %2
}

